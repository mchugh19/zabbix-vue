{
  "name": "__MSG_appName__",
  "version": "2.1.0",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "author": "https://github.com/mchugh19/zabbix-vue",
  "icons": {
    "128": "images/logo.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "permissions": [
    "background",
    "notifications",
    "storage",
    "*://*/"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_icon": {
      "128": "images/unconfigured.png"
    },
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "images/logo.png"
  ]
}
